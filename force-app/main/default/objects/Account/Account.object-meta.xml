    <validationRules>
        <fullName>Inherit_Primary_Contact_Billing_Address</fullName>
        <active>true</active>
        <description>This Validation Rule checks that a Primary Contact is selected, before a user can choose to Inherit the Primary Contact Billing Address.</description>
        <errorConditionFormula>AND(
    Inherit_PCBA__c = TRUE,
    (ISBLANK(Primary_Contact__c))
)</errorConditionFormula>
        <errorMessage>Please select a Primary Contact with an active Billing Address before enabling this checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inherit_Primary_Contact_Mailing_Address</fullName>
        <active>true</active>
        <description>This Validation Rule checks that a Primary Contact is selected, before a user can choose to Inherit the Primary Contact Mailing Address.</description>
        <errorConditionFormula>AND(
    Inherit_PCMA__c = TRUE,
    (ISBLANK(Primary_Contact__c))
)</errorConditionFormula>
        <errorMessage>Please select a Primary Contact with an active Mailing Address before enabling this checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Relationship_Inherit_Billing_Checked</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Primary_Contact__c)) &amp;&amp; ( Primary_Contact__r.Inherit_B_A_del__c  = true) &amp;&amp; 
(Inherit_PCBA__c = true)</errorConditionFormula>
        <errorDisplayField>Inherit_PCBA__c</errorDisplayField>
        <errorMessage>Inherit billing address at the Relationship&apos;s primary contact must be unchecked</errorMessage>
    </validationRules>
